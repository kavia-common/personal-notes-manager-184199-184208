# Frontend runtime variables
REACT_APP_API_BASE=http://localhost:8080
# Alternative legacy var if API_BASE is not set
REACT_APP_BACKEND_URL=http://localhost:8080
REACT_APP_WS_URL=
REACT_APP_LOG_LEVEL=warn
REACT_APP_HEALTHCHECK_PATH=/health
# Flags can be JSON or comma-separated list:
# e.g. REACT_APP_FEATURE_FLAGS='{"localStorageFallback":true,"showSettings":true}'
# or   REACT_APP_FEATURE_FLAGS=localStorageFallback,showSettings
REACT_APP_FEATURE_FLAGS=localStorageFallback,showSettings
